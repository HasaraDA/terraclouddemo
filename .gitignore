# Local .terraform directories (ignore in all subfolders)
**/.terraform/*

# .tfstate files
*.tfstate
*.tfstate.*

# Crash log files
crash.log
crash.*.log

# Sensitive variable files
*.tfvars
*.tfvars.json

# Ignore override files
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore transient lock info
.terraform.tfstate.lock.info

# Ignore Terraform lock file
.terraform.lock.hcl

# CLI configuration files
.terraformrc
terraform.rc

